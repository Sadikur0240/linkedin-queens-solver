{
    "manifest_version": 3,
    "name": "LinkedIn Queens Solver",
    "version": "1.1.0",
    "description": "Automatically solves LinkedIn Queens puzzles with universal compatibility for signed-in and signed-out users",
    "host_permissions": [
        "*://www.linkedin.com/games/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://www.linkedin.com/games/*",
                "*://www.linkedin.com/games/view/queens/desktop*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    "action": {
        "default_title": "LinkedIn Queens Solver",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "author": "Jason Xa",
    "homepage_url": "https://github.com/XaJason/linkedin-queens-solver",
    "minimum_chrome_version": "88"
}